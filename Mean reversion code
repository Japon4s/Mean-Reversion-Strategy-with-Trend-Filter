    "spy_bh = close.to_frame(name=\"Close\")\n",
    "spy_bh[\"return\"] = spy_bh[\"Close\"].pct_change()\n",
    "spy_bh[\"equity_bh\"] = (1 + spy_bh[\"return\"].fillna(0)).cumprod()\n",
    "\n",
    "pd.DataFrame({\n",
    "    \"buy_hold\": spy_bh[\"equity_bh\"],\n",
    "    \"your_strategy\": spy3[\"equity\"]\n",
    "}).dropna().plot(title=\"Buy&Hold vs Strategy\")"
   ]
