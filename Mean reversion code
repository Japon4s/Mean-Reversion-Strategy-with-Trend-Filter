   "source": [
    "compare = pd.DataFrame({\n",
    "    \"buy_hold\": spy_bh[\"equity_bh\"],\n",
    "    \"my_strategy\": spy3[\"equity\"]\n",
    "}).dropna()\n",
    "compare.head()\n",
    "split_date = \"2017-01-01\"\n",
    "train = compare.loc[:split_date]\n",
    "test  = compare.loc[split_date:]\n",
    "train.plot(title=\"TRAIN (until 2017): Buy&Hold vs Strategy\")\n",
    "test.plot(title=\"TEST (from 2017): Buy&Hold vs Strategy\")\n",
    "def stats(equity):\n",
    "    equity = equity.dropna()\n",
    "    final_equity = equity.iloc[-1]\n",
    "    max_dd = (equity / equity.cummax() - 1).min()\n",
    "    return final_equity, max_dd\n",
    "\n",
    "def board(label, values):\n",
    "    fe, dd = values\n",
    "    print(f\"{label:<15} | Equity: {fe:6.2f} | Max DD: {dd:6.2%}\")\n",
    "\n",
    "print(\"TRAIN\")\n",
    "pretty(\"Strategy\", stats(train[\"my_strategy\"]))\n",
    "pretty(\"Buy & Hold\", stats(train[\"buy_hold\"]))\n",
    "\n",
    "print(\"\\nTEST\")\n",
    "pretty(\"Strategy\", stats(test[\"my_strategy\"]))\n",
    "pretty(\"Buy & Hold\", stats(test[\"buy_hold\"]))"
   ]
