   "source": [
    "import yfinance as yf\n",
    "import pandas as pd\n",
    "import numpy as np\n",
    "\n",
    "spy = yf.download(\"SPY\", start=\"2010-01-01\")\n",
    "close = spy[\"Close\", \"SPY\"]\n",
    "\n",
    "spy3 = close.to_frame(name=\"Close\")\n",
    "\n",
    "spy3[\"return\"] = spy3[\"Close\"].pct_change()\n",
    "\n",
    "spy3[\"mom_6m\"] = spy3[\"Close\"].pct_change(126)\n",
    "\n",
    "spy3[\"signal\"] = (\n",
    "    (spy3[\"mom_6m\"] > 0) &\n",
    "    (spy3[\"return\"] <= -0.005)\n",
    ").astype(int)\n",
    "\n",
    "spy3[\"position\"] = spy3[\"signal\"].shift(1).rolling(5).max()\n",
    "\n",
    "spy3[\"strategy_return\"] = spy3[\"position\"] * spy3[\"return\"]\n",
    "\n",
    "spy3[\"equity\"] = (1 + spy3[\"strategy_return\"].fillna(0)).cumprod()\n",
    "spy3[\"equity\"].plot(title=\"SPY 6M Momentum Strategy (hold 2 days)\")"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 31,
