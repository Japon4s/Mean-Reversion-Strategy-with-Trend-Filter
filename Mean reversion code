    "final_equity = spy3[\"equity\"].iloc[-1]\n",
    "max_dd = (spy3[\"equity\"] / spy3[\"equity\"].cummax() - 1).min()\n",
    "time_in_market = spy3[\"position\"].mean()\n",
    "final_equity, max_dd, time_in_market\n"
   ]
